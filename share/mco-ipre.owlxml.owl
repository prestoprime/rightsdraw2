        <AnnotationProperty IRI="hasValidity"/>
        <IRI>urn:mpeg:mpeg21:cel:owl:2012#ActionRelatedFact</IRI>
    </AnnotationPropertyDomain>
    <AnnotationPropertyDomain>
        <AnnotationProperty IRI="hasValidity"/>
        <AbbreviatedIRI>mco-core:ActionRelatedFact</AbbreviatedIRI>
    </AnnotationPropertyDomain>
    <AnnotationPropertyDomain>
        <AnnotationProperty IRI="hasValidity"/>
        <IRI>Limited</IRI>
    </AnnotationPropertyDomain>
    <AnnotationPropertyDomain>
