    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <AbbreviatedIRI>dii:Identifier</AbbreviatedIRI>
        <Literal datatypeIRI="&xsd;string">it allows the identification information of a Digital Item</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>ActionDone</IRI>
        <Literal datatypeIRI="&xsd;string">Fact whose truth value depends on the accomplishment of Actions related to it by the mco-core:makesTrue property </Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>ActionRelatedFact</IRI>
        <Literal datatypeIRI="&xsd;string">Fact whose truth value depends on the status of Actions related to it by the 
mco-core:makesTrue property</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>ActionStarted</IRI>
        <Literal datatypeIRI="&xsd;string">Fact whose truth value depends on the beginning of Actions related to it by the mco-core:makesTrue property.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Address</IRI>
        <Literal datatypeIRI="&xsd;string">it attributes address information as plain text</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Authenticate</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Confirm the identity of a Content item in a value chain</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>ConsumeService</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Consume a Service.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Contract</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">the contract entity itself</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Deliver</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Transfer Content between Users of a value chain</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>DeonticExpression</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Defined class. An individual belongs to this class if it belongs to either celowl:Prohibition, celowl:Prohibition or mvco:Permission.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Describe</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Associate metadata to Content in a value chain</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>FactComposition</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Fact whose truth depends on one or more atomic facts, being an atomic fact the fact whose truth depends on no other fact</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>FactIntersection</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Fact  whose truth value will be evaluated as holding if any of the two Facts which must be related to it through an object property do also hold. It is restricted to being related with two and exactly two other Facts.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>FactNegation</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Fact  whose truth value will be evaluated as holding if the only Facts which must be related to it through does not hold. It is restricted to being related with one and only one other Fact.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>FactUnion</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Fact  whose truth value will be evaluated as holding if the two Facts which must be related to it through an object property do also hold. It is restricted to being related with two and exactly two other Facts</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Identify</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Assign Identifiers to Content in a value chain</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>InteractWith</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Trigger an action within a value chain through Content presented</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Obligation</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Clause binding one of the parties to execute an Action</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Organization</IRI>
        <Literal datatypeIRI="&xsd;string">for modelling a party represented in the contract by a signatory</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Package</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Make Content ready for delivery</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Post</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Let other Users access their Content</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Present</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Experience Content</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Process</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Perform operations on Content in a value chain</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Prohibition</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Clause binding one of the parties not to execute an Action</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>ProvideMaterial</IRI>
        <Literal datatypeIRI="&xsd;string">The action of providing at the disposal of (or making a delivery to)  a mvco:User or mco-core:Organization material related to an IPEntity (copies or instances)</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>ProvideService</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Offer a Service</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Service</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Transaction between two parties, where one of them performs a task for the second </Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Signature</IRI>
        <Literal datatypeIRI="&xsd;string">for expressing acceptance of the contract terms and identification of the signatory</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
